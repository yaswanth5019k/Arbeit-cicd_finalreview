# Ansible Secrets (DO NOT COMMIT - Add to .gitignore)
# Copy this file to secrets.yml and fill in your actual values

# SSH Key paths
ansible_ssh_private_key_file: ~/.ssh/id_rsa

# Sudo password for remote servers (if needed)
ansible_become_password: your_sudo_password

# Database credentials
db_password: your_db_password

# Application secrets
jwt_secret: your_jwt_secret
api_key: your_api_key

# Server IPs
tomcat_server_ip: 192.168.1.100
production_server_ip: 192.168.1.101
